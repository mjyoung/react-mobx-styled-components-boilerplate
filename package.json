{
  "name": "react-mobx-styled-components-boilerplate",
  "description": "Starter for creating React MobX apps with styled-components",
  "version": "1.0.0",
  "author": {
    "name": "Michael Young",
    "email": "michaeljy@gmail.com"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --progress --colors --config webpack.config.dev.js",
    "build": "npm run clean && cross-env NODE_ENV=production webpack  --config webpack.config.prod.js --progress --profile",
    "build-with-stats": "npm run clean && cross-env NODE_ENV=production webpack  --config webpack.config.prod.js --progress --profile --json > build/stats.json",
    "analyze": "webpack-bundle-analyzer build/stats.json",
    "build-analyze": "npm run build-with-stats && npm run analyze",
    "clean": "rimraf ./build && mkdirp ./build",
    "eslint": "eslint .",
    "eslint:fix": "eslint --fix .",
    "flow": "flow check",
    "prettier": "prettier --write \"**/*.{js,json,css,md}\"",
    "prettier:staged": "prettier --write \"**/*.js\""
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run flow && lint-staged"
    }
  },
  "pre-commit": [
    "prettier:staged",
    "flow",
    "eslint"
  ],
  "keywords": [
    "react",
    "mobx",
    "react-router",
    "webpack4"
  ],
  "license": "ISC",
  "dependencies": {
    "antd": "^3.6.1",
    "lodash": "^4.17.10",
    "mobx": "^4.3.0",
    "mobx-react": "^5.1.2",
    "prop-types": "^15.6.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-hot-loader": "^4.2.0",
    "react-router-dom": "^4.3.1",
    "regenerator-runtime": "^0.12.0",
    "styled-components": "^3.3.3"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-import": "^1.7.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^5.1.6",
    "css-loader": "^0.28.11",
    "eslint": "4.19.1",
    "eslint-config-airbnb": "16.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "2.11.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.7.0",
    "flow-bin": "^0.75.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.0.0-rc.9",
    "lint-staged": "^7.2.0",
    "mini-css-extract-plugin": "^0.4.1",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.9.0",
    "prettier": "^1.13.5",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "webpack": "^4.8.3",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4"
  }
}
